@model IEnumerable<DotNetCoreMVC_TaxCalculation.Models.Employee>

<table class="table">
    <thead>
        <tr>
            <th>EmpCode</th>
            <th>Name</th>
            <th>Designation</th>
            <th>Salary</th>
            <th>Tax</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.EmpCode</td>
                <td>@employee.Name</td>
                <td>@employee.Designation</td>
                <td>@employee.Salary</td>
                <td>@employee.Tax</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@employee.EmpCode">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>
